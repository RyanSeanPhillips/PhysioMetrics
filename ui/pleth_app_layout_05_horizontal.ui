    border-bottom-right-radius: 6px;
}

QToolButton#BrowseButton::menu-button:hover {
    background-color: #0b5ed7;
}

/* Make the arrow smaller */
QToolButton#BrowseButton::menu-arrow {
    width: 8px;
    height: 8px;
}
