QToolButton#BrowseButton:pressed {
    background-color: #0a58ca;
}

/* Keyboard focus ring */
QToolButton#BrowseButton:focus {
    outline: none;
    border: 2px solid #b6d4fe;   /* subtle focus ring */
}

/* Disabled */
QToolButton#BrowseButton:disabled {
    background-color: #9ec5fe;
    border-color: #9ec5fe;
    color: #eef4ff;
}

/* Split button dropdown arrow section - smaller width */
QToolButton#BrowseButton::menu-button {
    border-left: 2px solid rgba(255, 255, 255, 0.4);  /* Thicker, visible divider */
    width: 14px;  /* Smaller arrow area (was 18px) */
