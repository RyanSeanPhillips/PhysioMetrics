   <property name="styleSheet">
    <string notr="true">/* VS Code Dark+ Theme - Modified for true black */
QWidget {
    background-color: #000000;    /* True black for sharp appearance */
    color: #d4d4d4;               /* VS Code text color */
}

QLineEdit, QTextEdit, QPlainTextEdit {
    background-color: #252526;    /* VS Code input background */
    color: #cccccc;
    border: 1px solid #3e3e42;    /* VS Code border */
    border-radius: 3px;
    padding: 3px;
}
QLineEdit:focus, QTextEdit:focus, QPlainTextEdit:focus {
    border: 1px solid #007acc;    /* VS Code focus blue */
}

/* Buttons */
QPushButton {
    background-color: #2d2d30;    /* VS Code button background */
    color: #cccccc;
    border: 1px solid #3e3e42;
    padding: 5px;
    border-radius: 3px;
}
QPushButton:hover {
    background-color: #3e3e42;    /* VS Code button hover */
}
QPushButton:pressed {
    background-color: #094771;    /* VS Code button pressed */
}

/* Checkboxes */
#centralwidget QCheckBox {
    color: #cccccc;
}
#centralwidget QCheckBox:disabled {
    color: #6a6a6a;
}

/* Combo boxes ‚Äî match VS Code dropdowns */
#centralwidget QComboBox {
    background-color: #252526;
    color: #cccccc;
    border: 1px solid #3e3e42;
    padding: 3px 6px;
    border-radius: 3px;
}
#centralwidget QComboBox:hover {
    background-color: #2d2d30;
}
#centralwidget QComboBox:disabled {
    background-color: #000000;
    color: #6a6a6a;
    border-color: #2d2d30;
}

/* Drop-down button area */
#centralwidget QComboBox::drop-down {
    background: #2d2d30;
    border-left: 1px solid #3e3e42;
    width: 18px;
}

/* Popup list */
#centralwidget QComboBox QAbstractItemView {
    background: #252526;
    color: #cccccc;
    border: 1px solid #3e3e42;
    selection-background-color: #094771;  /* VS Code selection */
    selection-color: #ffffff;
    outline: 0;
}

/* If any combo is editable, make its inner editor match too */
#centralwidget QComboBox QLineEdit {
    background-color: #252526;
    color: #cccccc;
    border: none;
}

/* Checkbox indicator */
#centralwidget QCheckBox::indicator {
    width: 16px;
    height: 16px;
    border: 1px solid #cccccc;
    border-radius: 3px;
    background: #252526;
    margin-right: 3px;
}

#centralwidget QCheckBox::indicator:hover {
    border-color: #007acc;        /* VS Code blue on hover */
}

#centralwidget QCheckBox::indicator:checked {
    background: #007acc;          /* VS Code blue when checked */
    border-color: #007acc;
}

#centralwidget QCheckBox::indicator:disabled {
    border-color: #6a6a6a;
    background: #1e1e1e;
}
</string>
